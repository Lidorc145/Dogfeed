{
  "expo": {
    "name": "Dogfeel",
    "slug": "Dogfeel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dogSqureIcon.png",
    "splash": {
      "image": "./assets/dogIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationIcon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/short_bark.wav",
            "./assets/sounds/long_bark.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lidorc145.dogfeel",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "7",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/dogSqureIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [],
      "package": "com.lidorc145.dogfeel",
      "versionCode": 56
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bdd2bb29-097e-466f-a684-d67163507e91"
      }
    },
    "scheme": "dogfeel",
    "facebookScheme": "fb459488606352209"
  }
}
